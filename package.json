{
   "name": "user-auth-fitness",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "start": "tsc && node dist/index.js",
      "dev": "npm run migration:run && nodemon src/index.ts",
      "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
      "migration:generate": "npm run typeorm:cli -- migration:generate -d src/configuration/data-source.ts",
      "migration:run": "npm run typeorm:cli -- migration:run -d src/configuration/data-source.ts",
      "format": "prettier --write \"lib/**/*.ts\"",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "prettier": "prettier --write ./**/*.{ts,js,json} --ignore-path ./.prettierignore",
      "ts:verify": "tsc --noEmit",
      "verify": "npm run lint && npm run prettier && npm run ts:verify"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/express": "^5.0.0",
      "@types/jsonwebtoken": "^9.0.9",
      "@types/node": "^16.11.10",
      "express": "^4.21.2",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "argon2": "^0.41.1",
      "bcrypt": "^5.1.1",
      "class-validator": "^0.14.1",
      "dotenv": "^16.4.7",
      "eslint": "^9.21.0",
      "jsonwebtoken": "^9.0.2",
      "nodemon": "^3.1.9",
      "pg": "^8.4.0",
      "prettier": "^3.5.3",
      "reflect-metadata": "^0.1.13",
      "tsconfig-paths": "^4.2.0",
      "typeorm": "^0.3.21",
      "uuid": "^11.1.0"
   }
}
